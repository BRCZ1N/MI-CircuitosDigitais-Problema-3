//Finalizado
module modulo_contador_sync_7_bits_descendente(clr, clk, q, load);


	input clk,clr;
	input [6:0] load;
	wire [4:0]ff_inputs;
	output [6:0] q_bar;
	
	
	nand_gate_2_inputs gate_6(.A(load[6]),.B(),.S());
	nand_gate_2_inputs gate_7(.A(load[5]),.B(),.S());
	nand_gate_2_inputs gate_8(.A(load[4]),.B(),.S());
	nand_gate_2_inputs gate_9(.A(load[3]),.B(),.S());
	nand_gate_2_inputs gate_10(.A(load[2]),.B(),.S());
	nand_gate_2_inputs gate_11(.A(load[1]),.B(),.S());
	nand_gate_2_inputs gate_12(.A(load[0]),.B(),.S());
	
	nand_gate_2_inputs gate_13(.A(load[6]),.B(),.S());
	nand_gate_2_inputs gate_14(.A(load[5]),.B(),.S());
	nand_gate_2_inputs gate_15(.A(load[4]),.B(),.S());
	nand_gate_2_inputs gate_16(.A(load[3]),.B(),.S());
	nand_gate_2_inputs gate_17(.A(load[2]),.B(),.S());
	nand_gate_2_inputs gate_18(.A(load[1]),.B(),.S());
	nand_gate_2_inputs gate_19(.A(load[0]),.B(),.S());
	
	
	
	modulo_ff_t ff_1(.t(1'b1),.clk(clk),.clr(clr),.preset_n(1'b0),.clear_n(1'b0),.q(),.q_bar(q_bar[0]));
	
	modulo_ff_t ff_2(.t(q[0]),.clk(clk),.clr(clr),.preset_n(1'b0),.clear_n(1'b0),.q(),.q_bar(q_bar[1]));
	
	and_gate_2_inputs gate_1(.A(q_bar[0]),.B(q_bar[1]),.S(ff_inputs[0]));
	
	
	modulo_ff_t ff_3(.t(ff_inputs[0]),.clk(clk),.preset_n(1'b0),.clear_n(1'b0),.clr(clr),.q(),.q_bar(q_bar[2]));
	
	and_gate_2_inputs gate_2(.A(ff_inputs[0]),.B(q_bar[2]),.S(ff_inputs[1])); 
	
	
	modulo_ff_t ff_4(.t(ff_inputs[1]),.clk(clk),.preset_n(1'b0),.clear_n(1'b0),.clr(clr),.q(),.q_bar(q_bar[3]));
	
	and_gate_2_inputs gate_3(.A(ff_inputs[1]),.B(q_bar[3]),.S(ff_inputs[2])); 
	
	
	modulo_ff_t ff_5(.t(ff_inputs[2]),.clk(clk),.preset_n(1'b0),.clear_n(1'b0),.clr(clr),.q(),.q_bar(q_bar[4]));
	and_gate_2_inputs gate_4(.A(ff_inputs[2]),.B(q_bar[4]),.S(ff_inputs[3])); 
	
	
	modulo_ff_t ff_6(.t(ff_inputs[3]),.clk(clk),.preset_n(1'b0),.clear_n(1'b0),.clr(clr),.q(),.q_bar(q_bar[5]));
	and_gate_2_inputs gate_5(.A(ff_inputs[3]),.B(q_bar[5]),.S(ff_inputs[4])); 
	
	modulo_ff_t ff_7(.t(ff_inputs[4]),.clk(clk),.preset_n(1'b0),.clear_n(1'b0),.clr(clr),.q(),.q_bar(q_bar[6]));

	
endmodule 
	
	